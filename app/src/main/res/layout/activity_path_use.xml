    <Button
        android:id="@+id/cubicTo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cubicTo"/>

    <Button
        android:id="@+id/arcTo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="arcTo"/>

